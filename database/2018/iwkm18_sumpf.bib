  author    = {Sumpf, Jens and Bartsch, Ralf and Strobel, Jens and Bona, Marcus},
  title     = {Prozessdatenüberwachung in Kettenfördersystemen},
  booktitle = {Scientific Reports},
  year      = {2018},
